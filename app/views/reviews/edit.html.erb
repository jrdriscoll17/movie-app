<h1><%= @review.movie.title %></h1>
<%= form_for([@review.user, @review]) do |f| %>
  <%= f.label :rating %>
  <%= f.number_field :rating %><br>
  <%= f.label :content %><br>
  <%= f.text_area :content %><br>
  <%= f.submit %>
  <%= button_to 'delete', user_review_path(@review.user, @review), method: :delete %>
<% end %>
